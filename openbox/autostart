#fi

# If you want to use XFCE config tools...
#
#xfce-mcs-manager &

devmon &


redshift -l 39.98389:-75.14986 -t 6500:5000 &

wicd-gtk &

if [ $(xrandr -q | grep -c "HDMI-0 disconnected" ) -ne 0 ]
then
	xrandr --output LVDS --mode 1366x768 &
	(sleep 0.5 && conky -c ~/.conkylaptoprc) &
	#sonata &
else
	xset s off &
	xrandr --output LVDS --off &
	xrandr --output HDMI-0 --mode 1920x1080 & 
	(sleep 0.5 && conky) &
	#leafpad &
fi

(sleep 1 && ~/.fehbg) & 

thunar --daemon &

#mpd &

stalonetray &

/usr/bin/amixer -c 0 sset "Auto-Mute Mode" Enabled

dropboxd &

#google-musicmanager &
