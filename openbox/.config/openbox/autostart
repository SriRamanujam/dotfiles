~/.fehbg &

conky --daemon &

compton -CGcf -t -4 -l -2 -r4 -o .55 -D2 --backend glx --glx-use-copysubbuffermesa --paint-on-overlay --glx-no-stencil --glx-no-rebind-pixmap --vsync opengl-swc &

if [ $(xrandr -q | grep -c "HDMI-0 disconnected" ) -eq 0 ]
then
	xset s off &
	xset -dpms &
fi

