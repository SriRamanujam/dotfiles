#!/bin/bash

#
# Managed by Ansible. Do not edit.
#

# Export term to be something with 256 colors
export TERM=xterm-256color

# Source all the dropins from the various configs
for f in {{ bash_config_dropins_dir }}/*; do source $f; done

# set up PATH so that our binaries take precedence over everything else
if ! [[ "$PATH" =~ "{{ local_bin_dir }}" ]]
then
    PATH="{{ local_bin_dir }}:$PATH"
fi
export PATH
